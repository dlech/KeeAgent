<?xml version="1.0" encoding="utf-8"?>
<Configuration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Meta>
    <!-- Don't mess up user config! -->
    <PreferUserConfiguration>false</PreferUserConfiguration>
    <OmitItemsWithDefaultValues>true</OmitItemsWithDefaultValues>
  </Meta>
  <!-- Use all defaults with a few exceptions -->
  <Application>
    <Start>
      <!-- Don't try to do anything on startup -->
      <OpenLastFile>false</OpenLastFile>
      <CheckForUpdate>false</CheckForUpdate>
      <CheckForUpdateConfigured>true</CheckForUpdateConfigured>
    </Start>
  </Application>
  <MainWindow>
		<EntryListColumnCollection>
			<Column>
				<Type>Title</Type>
				<Width>90</Width>
			</Column>
			<Column>
				<Type>PluginExt</Type>
				<CustomName>SSH Key Status</CustomName>
				<Width>90</Width>
			</Column>
			<Column>
				<Type>UserName</Type>
				<Width>90</Width>
			</Column>
			<Column>
				<Type>Password</Type>
				<Width>90</Width>
				<HideWithAsterisks>true</HideWithAsterisks>
			</Column>
			<Column>
				<Type>Url</Type>
				<Width>90</Width>
			</Column>
		</EntryListColumnCollection>
  </MainWindow>
  <Integration>
    <LimitToSingleInstance>false</LimitToSingleInstance>
  </Integration>
	<Custom>
		<Item>
			<Key>KeeAgent.AlwaysConfirm</Key>
			<Value>false</Value>
		</Item>
		<Item>
			<Key>KeeAgent.ShowBalloon</Key>
			<Value>true</Value>
		</Item>
		<Item>
			<Key>KeeAgent.AgentMode</Key>
			<Value>Auto</Value>
		</Item>
		<Item>
			<Key>KeeAgent.UnlockOnActivity</Key>
			<Value>true</Value>
		</Item>
		<Item>
			<Key>KeeAgent.UseCygwinSocket</Key>
			<Value>false</Value>
		</Item>
		<Item>
			<Key>KeeAgent.UseMsysSocket</Key>
			<Value>false</Value>
		</Item>
		<Item>
			<Key>KeeAgent.UseWindowsOpenSshPipe</Key>
			<Value>false</Value>
		</Item>
		<Item>
			<Key>KeeAgent.UnixSocketPath</Key>
			<Value></Value>
		</Item>
		<Item>
			<Key>KeeAgent.UserPicksKeyOnRequestIdentities</Key>
			<Value>false</Value>
		</Item>
		<Item>
			<Key>KeeAgent.IgnoreMissingExternalKeyFilesName</Key>
			<Value>false</Value>
		</Item>
	</Custom>
</Configuration>
